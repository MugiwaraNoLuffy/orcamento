<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.redir.orcamento" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-sdk android:minSdkVersion="11" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name=".AuthActivities.RegisterActivity"
            android:excludeFromRecents="true"
            android:label="@string/title_activity_register"
            android:parentActivityName=".AuthActivities.LoginActivity" >
        </activity>
        <activity
            android:name=".AuthActivities.LoginActivity"
            android:excludeFromRecents="true"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".MenuActivitiesTest.MenuActivity"
            android:label="@string/title_activity_cadastro"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".MenuFragments.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
